    "title": "<%= locals[:title] %>",
    "user": {
      "login": "<%= locals[:user][:login] %>",
      "id": "<%= locals[:user][:id] %>",
      "avatar_url": "<%= locals[:user][:avatar_url] %>",
      "gravatar_id": "",
      "type": "User",
